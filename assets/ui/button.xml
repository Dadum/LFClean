<Ui xsi:schemaLocation="http://www.blizzard.com/wow/ui/ ..\FrameXML\UI.xsd">
    <Button name="LFClean_ReportButton" virtual="true">
        <Size x="32" y="32" />
        <NormalTexture file="Interface\BUTTONS\UI-SquareButton-Up" /> 
        <PushedTexture file="Interface\BUTTONS\UI-SquareButton-Down" />
        <DisabledTexture file="Interface\BUTTONS\UI-SquareButton-Disabled" />
        <HighlightTexture file="Interface\BUTTONS\UI-Common-MouseHilight" alphaMode="ADD" />
        <Layers>
            <Layer level="ARTWORK">
                <Texture parentKey="Icon" file="Interface\Addons\LFClean\assets\textures\ReportIcon">
                    <Size x="16" y="16" />
                    <Anchors>
						<Anchor point="CENTER" x="-1" y="0"/>
					</Anchors>
                </Texture>
            </Layer>
        </Layers>
    </Button>
</Ui>